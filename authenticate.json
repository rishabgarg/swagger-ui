{
    "swagger": "2.0",
    "info": {
        "title": "Zillion Developer API",
        "description": "Move your app forward with the Zillion API",
        "version": "1.0.0"
    },
    "host": "api.zillion.com",
    "schemes": [
        "https"
    ],
    "basePath": "/",
    "produces": [
        "application/json"
    ],
    "paths": {
        "/auth/v2/user": {
            "post": {
                "summary": "Authenticate User",
                "description": "The Products endpoint returns information about the *Uber* products\noffered at a given location. The response includes the display name\nand other details about each product, and lists the products in the\nproper display order.\n",
                "parameters": [
                    {
                        "name": "authorizedSignatureToken",
                        "in": "header",
                        "description": "Generated signature token (you get this when you successfully log in and authenticate your application.)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "applicationName",
                        "in": "header",
                        "description": "Name of the application you're trying to call API of (there are many applications in Platform OS.)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "organizationID",
                        "in": "header",
                        "description": "Your organization's id. You get this when you register to use our Platform OS APIs.",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "uniqueRequestID",
                        "in": "header",
                        "description": "A random number generated by your system.",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "authorizedUserId",
                        "in": "header",
                        "description": "Id of the user logged in and trying to call this API.",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "TimeInUTC",
                        "in": "header",
                        "description": "Current time the request is made, in UTC. Note that this time has to be within +/- 5 minutes of current UTC time.",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "user",
                        "in": "body",
                        "description": "Latitude component of location.",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/UserCredentials"
                        }
                    }
                ],
                "tags": [
                    "Authenticate User"
                ],
                "responses": {
                    "200": {
                        "description": "An array of products",
                        "schema": {
                            "$ref": "#/definitions/Success"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "UserCredentials": {
            "type": "object",
            "properties": {
                "username": {
                    "type": "string",
                    "description": "Username to authenticate."
                },
                "password": {
                    "type": "string",
                    "description": "Password associated with username to authenticate."
                },
                "userId": {
                    "type": "string",
                    "description": "Username to authenticate."
                },
                "timeOfRequestInUTC": {
                    "type": "string",
                    "description": "Datetime value (yyyy-MM-ddThh:mm:ss.SSSZ) of what the time is when making the request."
                },
                "expirationDateTimeInUTC": {
                    "type": "string",
                    "description": "Datetime value (yyyy-MM-ddThh:mm:ss.SSSZ) of what the time is when making the request."
                },
                "signatureToken": {
                    "type": "string",
                    "description": "Datetime value (yyyy-MM-ddThh:mm:ss.SSSZ) of what the time is when making the request."
                }
            }
        },
        "Success": {
            "type": "object",
            "properties": {
                "ResponseStatus": {
                    "type": "string"
                },
                "ResponseMessage": {
                    "type": "string"
                },
                "ResponseCode": {
                    "type": "string"
                },
                "UniqueRequestId": {
                    "type": "string"
                },
                "ResponseData": {
                    "$ref": "#/definitions/UserCredentials"
                }
            }
        },
        "Error": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "message": {
                    "type": "string"
                },
                "fields": {
                    "type": "string"
                }
            }
        }
    }
}